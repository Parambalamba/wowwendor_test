{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wow/wow-test",
	"version": "0.1.0",
	"title": "Wow Test Poke Block",
	"category": "design",
	"icon": "superhero-alt",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "wow-test",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
  	"render": "file:./render.php",
	"viewScript": "file:./view.js",
  	"attributes": {
		"pokemon_types": {
		  "type": "string",
		  "default": "https://pokeapi.co/api/v2/type/1/"
		},
	  	"type_name":{
		  "type": "string",
		  "default": "normal"
		},
	  	"pokes_html": {
		  "type": "html"
		}
	}
}
